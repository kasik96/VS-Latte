						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
							<string>keyword.other.latte</string>
						</dict>
					</dict>
					<key>match</key>
					<string>.*?(\|)(\w+)(?:\:([^\|\}]+))*</string>
				</dict>
				<dict>
					<key>include</key>
					<string>#strings</string>
