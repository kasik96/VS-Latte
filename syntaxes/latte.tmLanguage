		<dict>
			<key>begin</key>
			<string>\{\*</string>
			<key>end</key>
			<string>\*\}</string>
			<key>name</key>
			<string>comment.block.latte</string>
		</dict>
		<dict>
			<key>begin</key>
			<string>\{\{?(?!\s)</string>
